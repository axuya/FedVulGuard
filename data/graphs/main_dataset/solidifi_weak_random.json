        "node_type": "statement",
        "content": "bytes32 randHash = keccak256(abi.encode(seed1, seed2, seed3));",
        "id": "solidifi_weak_random_stmt_6"
      },
      {
        "node_type": "statement",
        "content": "uint winningNumber = uint(randHash) % totalTickets;",
        "id": "solidifi_weak_random_stmt_7"
      },
      {
        "node_type": "statement",
        "content": "address payable winningAddress = contestants[winningNumber].addr;",
        "id": "solidifi_weak_random_stmt_8"
      },
      {
        "node_type": "statement",
        "content": "gameId++;",
        "id": "solidifi_weak_random_stmt_9"
      },
      {
