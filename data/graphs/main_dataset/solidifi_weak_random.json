      },
      {
        "node_type": "statement",
        "content": "address seed1 = contestants[uint(block.coinbase) % totalTickets].addr;",
        "id": "solidifi_weak_random_stmt_3"
      },
      {
        "node_type": "statement",
        "content": "address seed2 = contestants[uint(msg.sender) % totalTickets].addr;",
